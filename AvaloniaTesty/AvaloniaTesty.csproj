   </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\ClassLibraryTesty\ClassLibraryTesty.csproj" />
  </ItemGroup>
  <ItemGroup>
    <Folder Include="Dlgs\" />
  </ItemGroup>
  <ItemGroup>
    <None Update="SideBar.xaml">
      <Generator>MSBuild:Compile</Generator>
    </None>
  </ItemGroup>
</Project>
